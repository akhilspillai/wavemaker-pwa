{
	"$schema": "http://json-schema.org/schema",
	"id": "WavemakerPwaSchema",
	"type": "object",
	"properties": {
		"iconsPath": {
			"type": "string",
			"format": "path",
			"description": "The path to an external icons directory."
		},
		"project": {
			"type": "string",
			"description": "The name of the project.",
			"$default": {
				"$source": "projectName"
			}
		},
		"title": {
			"type": "string",
			"description": "The title of the app."
		},
		"themeColor": {
			"type": "string",
			"description": "The theme color of the app.",
			"default": "#2c3049"
		},
		"deployUrl": {
			"type": "string",
			"description": "The url where the app is deployed.",
			"default": "ng-bundle"
		}
	}
}